class Ex {
	public static int x(int a) {return a*100;}
	public static int y(int a) {return a*200;}
	public static int z(int a) {return a*300;}
}
public class Ex01 {
	public static void main(String[] args) {
		System.out.println(Ex.x(3));
		System.out.println(Ex.y(2));
		System.out.println(Ex.z(6));
	}

}
